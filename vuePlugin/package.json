{
    "name": "vue-plugin",
    "scripts": {
        "build": "browserify script -o main.js -t [ babelify --presets [ es2015 ] ]"
    },
    "version": "0.0.1",
    "devDependencies": {
        "babel-preset-es2015": "^6.24.1",
        "babelify": "^7.3.0",
        "browserify": "^16.5.1",
        "vue": "^2.6.11"
    },
    "browserify": {
        "transform": [
            [
                "babelify",
                {
                    "presets": [
                        "es2015"
                    ]
                }
            ]
        ]
    },
    "dependencies": {
        "build": "^0.1.4",
        "vuex": "^3.1.3"
    }
}
